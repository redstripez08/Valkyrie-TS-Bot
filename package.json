{
  "name": "valkyrie-ts-bot",
  "version": "1.0.4",
  "description": "Valkyrie Discord Bot",
  "main": "./dist/index.js",
  "scripts": {
    "start": "SET NODE_ENV=production && --unhandled-rejections=strict node .",
    "dev:start": "SET NODE_ENV=dev && node -r dotenv/config --trace-deprecation --trace-warnings --trace-uncaught --unhandled-rejections=warn .",
    "ts:start": "SET NODE_ENV=dev && ts-node -r dotenv/config ./src/index.ts",
    "tsc:build": "tsc",
    "tsc:watch": "tsc --watch"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redstripez08/CalNatSci-Discord-Bot-TS.git"
  },
  "author": "Angelo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/redstripez08/CalNatSci-Discord-Bot-TS/issues"
  },
  "homepage": "https://github.com/redstripez08/CalNatSci-Discord-Bot-TS#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^12.5.1",
    "keyv": "^4.0.3",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.167",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.20",
    "@types/sequelize": "^4.28.9",
    "dotenv": "^8.2.0"
  },
  "_globalDependencies": {
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
